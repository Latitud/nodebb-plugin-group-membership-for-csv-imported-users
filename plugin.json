{
	"id": "nodebb-plugin-group-membership-for-user-csv-importing",
	"url": "https://github.com/Latitud/nodebb-plugin-group-membership-for-user-csv-importing",
	"library": "./library.js",
	"hooks": [
		{ "hook": "action:importUsersCSV.created", "method": "createGroupMemberships" },
		{ "hook": "static:app.load", "method": "init" },
